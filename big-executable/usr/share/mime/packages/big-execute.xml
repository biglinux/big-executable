<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-executable">
    <comment>Executable</comment>
    <comment xml:lang="pt">Ficheiro executável</comment>
    <comment xml:lang="pt_BR">Arquivo executável</comment>
    <generic-icon name="application-x-executable"/>
    <magic priority="50">
      <match value="EXECUTABLE" type="string" offset="0"/>
      <match value="EXECUTABLE" type="string" offset="16"/>
    </magic>
    <glob pattern="*.bin"/>
  </mime-type>
</mime-info>
